{
	"manifest_version": 2,
	"version": "0.0.1",
	"name": "myFM",
	"description": "A FM raido base on douban.fm",
	"icons": {
	},
	"permissions": [ "*://*/*", "storage" ],
	"app": {
		"background": {
			"scripts": [ "js/ds.js", "js/background.js" ]
		}
	},
	"commands": {
		"play_pause": {
			"suggested_key": {
				"default": "MediaPlayPause"
			},
			"description": "Play or pause",
			"global": true
		},
		"next": {
			"suggested_key": {
				"default": "MediaNextTrack"
			},
			"description": "Next Music",
			"global": true
		},
		"prev": {
			"suggested_key": {
				"default": "MediaPrevTrack"
			},
			"description": "Previous Music",
			"global": true
		},
		"volume_up": {
			"suggested_key": {
				"default": "Ctrl+Shift+P"
			},
			"description": "Volume up",
			"global": true
		},
		"volume_down": {
			"suggested_key": {
				"default": "Ctrl+Shift+O"
			},
			"description": "Volume down",
			"global": true
		},
		"test_hot_key": {
			"suggested_key": {
				"default": "MediaStop"
			},
			"description": "Test hot key222",
			"global": true
		}
	}
}